= content_for :head do
  = javascript_include_tag params[:controller], 'data-turbolinks-track': 'reload'
.background-wrapper
  .container.position-relative
    .row
      .col-6.pt-5
        h1.text-white.display-4.fw-bold = t ".title"
        p.h4.text-white.text-muted = t ".sub_title"
      .col-6
        = render 'search'
